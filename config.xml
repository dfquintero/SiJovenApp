<?xml version="1.0" encoding="UTF-8"?>

<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.ColombiaJoven.SiJoven"
        version   = "1.0.0">

  <name>Sí Joven App</name>

  <description>
    Aplicación de servicios integrados para el joven
  </description>

  <author href="http://dfquintero.com" email="soporte@dfquintero.com">
    dfquintero - Marylin Alarcon 
  </author>

  <preference name="permissions"                value="none"/>

  <!-- Customize your app and platform with the preference element. -->
  <preference name="phonegap-version"           value="3.5.0" />          <!-- all: current version of PhoneGap -->
  <preference name="orientation"                value="portrait" />        <!-- all: default means both landscape and portrait are enabled -->
  <preference name="target-device"              value="universal" />      <!-- all: possible values handset, tablet, or universal -->
  <preference name="fullscreen"                 value="true" />           <!-- all: hides the status bar at the top of the screen -->

  <!-- Plugins -->

  <!-- Core plugins -->
  <gap:plugin name="org.apache.cordova.device-orientation" />
  <gap:plugin name="org.apache.cordova.splashscreen" />
  
  <gap:plugin name="nl.x-services.plugins.socialsharing" version="4.3.0" />
  <gap:plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay" />

  <gap:plugin name="com.phonegap.plugin.statusbar" />
  <preference name="StatusBarOverlaysWebView" value="false" />
  
  <gap:config-file platform="ios" parent="UIStatusBarHidden">
    <true/>
  </gap:config-file>

  <gap:config-file platform="ios" parent="UIViewControllerBasedStatusBarAppearance">
    <false/>
  </gap:config-file>
  

  <!-- Define app icon for each platform. -->

  <!--iOS 6.1-->
  <!-- iPhone / iPod Touch -->
  <icon src="res/ico/ios/ico_ios_57.png" gap:platform="ios" width="57" height="57" />
  <icon src="res/ico/ios/ico/ios/ico_ios_114.png" gap:platform="ios" width="114" height="114" />
  <!-- iPad -->
  <icon src="res/ico/ios/ico_ios_72.png" gap:platform="ios" width="72" height="72" />
  <icon src="res/ico/ios/ico_ios_144.png" gap:platform="ios" width="144" height="144" />
  <!-- iPhone Spotlight and Settings Icon -->
  <icon src="res/ico/ios/ico_ios_29.png" gap:platform="ios" width="29" height="29" />
  <icon src="res/ico/ios/ico_ios_58.png" gap:platform="ios" width="58" height="58" />
  <!-- iPad Spotlight and Settings Icon -->
  <icon src="res/ico/ios/ico_ios_50.png" gap:platform="ios" width="50" height="50" />
  <icon src="res/ico/ios/ico_ios_100.png" gap:platform="ios" width="100" height="100" />
  <!--iOS 7.0+-->
  <!-- iPhone / iPod Touch  -->
  <icon src="res/ico/ios/ico_ios_60.png" gap:platform="ios" width="60" height="60" />
  <icon src="res/ico/ios/ico_ios_120.png" gap:platform="ios" width="120" height="120" />
  <!-- iPad -->
  <icon src="res/ico/ios/ico_ios_76.png" gap:platform="ios" width="76" height="76" />
  <icon src="res/ico/ios/ico_ios_152.png" gap:platform="ios" width="152" height="152" />
  <!-- Settings Icon -->
  <icon src="res/ico/ios/ico_ios_29.png" gap:platform="ios" width="29" height="29" />
  <icon src="res/ico/ios/ico_ios_58.png" gap:platform="ios" width="58" height="58" />
  <!-- Spotlight Icon -->
  <icon src="res/ico/ios/ico_ios_40.png" gap:platform="ios" width="40" height="40" />
  <icon src="res/ico/ios/ico_ios_80.png" gap:platform="ios" width="80" height="80" />

  <!--Android-->
  <icon src="res/ico/Android/ico_ldpi_36.png" gap:platform="android" gap:qualifier="ldpi" />
  <icon src="res/ico/Android/ico_mdpi_48.png" gap:platform="android" gap:qualifier="mdpi" />
  <icon src="res/ico/Android/ico_hdpi_72.png" gap:platform="android" gap:qualifier="hdpi" />
  <icon src="res/ico/Android/ico_xhdpi_96.png" gap:platform="android" gap:qualifier="xhdpi" />
  <icon src="res/ico/Android/ico_xxhdpi_144.png" gap:platform="android" gap:qualifier="xxhdpi" />
  <icon src="res/ico/Android/ico_xxxhdpi_192.png" gap:platform="android" gap:qualifier="xxxhdpi" />
  <icon src="ico.png" />

  <!-- Define app splash screen for each platform. -->
  <!-- iPhone and iPod touch -->
  <gap:splash src="res/Splash/ios/480.png" gap:platform="ios" width="320" height="480" />
  <gap:splash src="res/Splash/ios/960.png" gap:platform="ios" width="640" height="960" />
  <!-- iPhone 5 / iPod Touch (5th Generation) -->
  <gap:splash src="res/Splash/ios/1136.png" gap:platform="ios" width="640" height="1136" />
  <!-- iPad -->
  <gap:splash src="res/Splash/ios/1024.png" gap:platform="ios" width="768" height="1024" />
  <!-- Retina iPad -->
  <gap:splash src="res/Splash/ios/2048.png" gap:platform="ios" width="1536" height="2048" />
  <!--Android-->
  <gap:splash src="res/Splash/Android/ldpi.png" gap:platform="android" gap:qualifier="ldpi" />
  <gap:splash src="res/Splash/Android/mdpi.png" gap:platform="android" gap:qualifier="mdpi" />
  <gap:splash src="res/Splash/Android/hdpi.png" gap:platform="android" gap:qualifier="hdpi" />
  <gap:splash src="res/Splash/Android/xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />

  <!--<gap:splash src="res/screen/windows-phone/screen-portrait.jpg"      gap:platform="winphone" />-->
  <!--<gap:splash src="res/screen/blackberry/screen-225.png"              gap:platform="blackberry" />-->
  
  <gap:splash src="splash.png" />
  
  <access origin="*" /> - a wildcard access tag allows access to all external resource.

</widget>

